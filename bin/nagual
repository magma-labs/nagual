#!/usr/bin/env ruby
require 'nagual'

if ARGV.first == 'export'
  File.open(Nagual::Configuration.properties['output_file'], 'w') do |file|
    file.write(Nagual.export(Nagual::Configuration.properties['input_file']))
  end
elsif ARGV.first == 'review'
  puts Nagual.review(Nagual::Configuration.properties['input_file'])
else
  puts 'Command not recognized'
end
